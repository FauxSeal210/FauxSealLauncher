        Title="로스트사가 런쳐" Height="204.277" Width="425.044" ResizeMode="CanMinimize" Loaded="Window_Loaded">
    <Grid HorizontalAlignment="Left" Height="397" VerticalAlignment="Top" Width="598" Margin="10,0,0,-19">
        <osm:WebControl x:Name="webControl" Margin="402,28,146,319" Visibility="Hidden" />
        <TextBox x:Name="ID" HorizontalAlignment="Left" Height="26" Margin="11,11,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="250" materialDesign:HintAssist.Hint="ID"/>
        <PasswordBox x:Name="PW" HorizontalAlignment="Left" Margin="11,53,0,0" VerticalAlignment="Top" Width="250" materialDesign:HintAssist.Hint="PW"/>
        <Button x:Name="Start" Content="Start" HorizontalAlignment="Left" Margin="11,83,0,0" VerticalAlignment="Top" Width="85" IsEnabled="False" Click="Start_Click"/>
        <Button x:Name="LoGin" Content="Login" HorizontalAlignment="Left" Margin="285,11,0,0" VerticalAlignment="Top" Width="98" Height="70" Click="Button_Click"/>
        <Label Content="Made by FauxSeal210 (로스트사가: ExtraTerrestrial)" HorizontalAlignment="Left" Margin="96,87,0,0" VerticalAlignment="Top" Width="315" Height="28"/>
        <RadioButton x:Name="Nexon" Content="Nexon" HorizontalAlignment="Left" Margin="10,120,0,0" VerticalAlignment="Top" Width="63"/>
        <RadioButton x:Name="Naver" Content="Naver" HorizontalAlignment="Left" Margin="92,120,0,0" VerticalAlignment="Top"/>
        <RadioButton x:Name="Daum" Content="Daum" HorizontalAlignment="Left" Margin="170,120,0,0" VerticalAlignment="Top"/>
        <CheckBox x:Name="Favorite" Content="기본 서버로 등록" HorizontalAlignment="Left" Margin="263,120,0,0" VerticalAlignment="Top"/>
        <Button Content="런쳐 바로가기 생성" HorizontalAlignment="Left" Margin="203,143,0,0" VerticalAlignment="Top" Width="190" Height="26" Click="Button_Click_1"/>
        <Button Content="런쳐 소스 구경하기" HorizontalAlignment="Left" Margin="10,143,0,0" VerticalAlignment="Top" Width="188" Height="26" Click="Button_Click_2"/>
    </Grid>
</Window>
